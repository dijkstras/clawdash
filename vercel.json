{
  "version": 2,
  "name": "sjoerd-dashboard",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/news.json",
      "dest": "/news.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}